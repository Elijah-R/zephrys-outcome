{
  "name": "zephrys-helper",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "npm run test:pug && npm run test:style && npm run test:js",
    "test:pug": "glob-exec \"source/**/*.pug\" -- \"pug-lint {{files.join(' ')}}\"",
    "test:style": "stylelint \"source/blocks/**/*.scss\" --syntax scss",
    "test:js": "eslint source/blocks/**/*.js",
    "build": "gulp build",
    "start": "gulp start"
  },
  "lint-staged": {
    "source/blocks/**/*.{js,pug,md}": "editorconfig-cli",
    "*.pug": "pug-lint",
    "source/blocks/**/*.scss": "stylelint --syntax scss",
    "source/blocks/**/*.js": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "#"
  },
  "author": "me",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^9.7.5",
    "browser-sync": "^2.26.7",
    "del": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "glob-exec": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pretty-html": "^2.0.10",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-svgstore": "^7.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "node-normalize-scss": "^8.0.1",
    "pug-lint": "^2.6.0",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "stylelint": "^13.2.1",
    "stylelint-at-rule-no-children": "^0.3.1",
    "stylelint-config-htmlacademy": "^0.1.4"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "editorconfig-cli": [
    "source/blocks/**/*.js",
    "source/blocks/**/*.scss"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
